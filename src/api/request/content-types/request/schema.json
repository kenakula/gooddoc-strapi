{
  "kind": "collectionType",
  "collectionName": "requests",
  "info": {
    "singularName": "request",
    "pluralName": "requests",
    "displayName": "requests"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "connectionType": {
      "type": "enumeration",
      "enum": [
        "phone",
        "watsapp",
        "telegram"
      ],
      "required": true,
      "default": "phone"
    },
    "comment": {
      "type": "text",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "clinic",
        "doctor"
      ],
      "default": "doctor",
      "required": true
    },
    "clinic": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::clinic.clinic"
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::doctor.doctor"
    }
  }
}
