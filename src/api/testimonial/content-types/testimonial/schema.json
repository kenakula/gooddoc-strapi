{
  "kind": "collectionType",
  "collectionName": "testimonials",
  "info": {
    "singularName": "testimonial",
    "pluralName": "testimonials",
    "displayName": "Testimonials"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "showOnMainPage": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "rate": {
      "type": "integer",
      "required": true,
      "default": 5,
      "max": 5,
      "min": 1
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "app",
        "doctor",
        "clinic",
        "insurance"
      ]
    },
    "author": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "comment": {
      "type": "text"
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::doctor.doctor"
    },
    "clinic": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::clinic.clinic"
    },
    "insurance": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::insurance.insurance"
    }
  }
}
